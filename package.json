{
  "name": "code-pipeline-deploy",
  "version": "2.3.0",
  "description": "Code Pipeline github action deploy",
  "main": "index.js",
  "scripts": {
    "build": "ncc build index.js -o dist",
    "lint": "eslint .",
    "format": "prettier --write **/*.js",
    "format-check": "prettier --check **/*.js",
    "prepare": "ncc build index.js -o dist --source-map --license licenses.txt",
    "test": "nyc --all=true --exclude=\"dist/**\" --exclude=\"coverage/**\" --exclude=\"test/**\" --reporter=json --reporter=text mocha",
    "all": "npm run lint && npm run prepare && npm run test"
  },
  "repository": {
    "type": "git",
     "url": "git+https://github.com/bmc-compuware/ispw-deploy.git"
  },
 "keywords": [
    "ISPW",
    "ispw-build",
    "BMC",
    "Compuware",
    "actions",
    "Code Pipeline",
    "AMI"
  ],
  "author": "BMC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bmc-compuware/ispw-deploy/issues"
  },
  "homepage": "https://github.com/marketplace/actions/code-pipeline-deploy",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@bmc-compuware/ispw-action-utilities": "https://github.com/bmc-compuware/ispw-action-utilities.git#main",
    "chai": "^4.3.4",
    "github": "^14.0.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.34.0",
    "eslint": "^7.27.0",
    "jest": "^27.0.1"
  }
}
